<table class="mainTable">
    <tr class="table">
        <th>#</th>
        <th>Name</th>
        <th>Last</th>
        <th>Buy / Sell Price</th>
        <th>Volume</th>
        <th>Base_unit</th>
    </tr>
    <%let count1= 0
    for(let value of values){
        count1=count1+1%>

        <tr class="tableValue">
            <td class="leftRadius"><%=count1%></td>
            <td><%=value.name%></td>
            <td>&#8377; <%=value.last%></td>
            <td>&#8377; <%=value.buy%> / &#8377; <%=value.sell%></td>
            <td><%=value.volume%></td>
            <td class="rightRadius"><%=value.base_unit%></td>
        </tr>
    <%}%>
</table>